<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-3.1.xsd
	">
	
	<bean id="username" class="java.lang.String">
		<constructor-arg value="My Name" />
	</bean>
	
	<bean id="password" class="java.lang.String">
		<constructor-arg value="MyPassword" />
	</bean>
	
	<bean id="firstUser" class="com.fdm.springCoreIntro.User" init-method="complete">
		<constructor-arg ref="username" />
		<constructor-arg ref="password" />
	</bean>
	
	<bean id="thirdUser" class="com.fdm.springCoreIntro.User" init-method="complete">
		<constructor-arg value="6" />
		<constructor-arg value="My Address" />
	</bean>
	
	<bean id="fourthUser" class="com.fdm.springCoreIntro.User" init-method="complete">
		<constructor-arg value="6" type="int" index="0" />
		<constructor-arg value="My Address" type="java.lang.String" index="1" />
	</bean>
	
	<bean id="secondUser" class="com.fdm.springCoreIntro.User" init-method="complete">
		<property name="username" ref="username" />
		<property name="password" ref="password" />
	</bean>
	

</beans>






